@{
}
<div class="row">
    @foreach(Laptop laptop in Model){
        <div class="col-md-6">
            <p> Model: @laptop.Model</p>
            <p>Price: @laptop.Price</p>
            <p>Id: @laptop.Id</p>
            <p>Year: @laptop.Year  </p>
            <p> Model: @laptop.Brand.Name</p>
        </div>
    }

</div>